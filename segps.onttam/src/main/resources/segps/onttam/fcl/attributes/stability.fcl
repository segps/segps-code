FUNCTION_BLOCK OVERALL

VAR_INPUT
	BCD_Score: REAL;
	NSD_Score: REAL;
END_VAR

VAR_OUTPUT
	score: REAL;
END_VAR

FUZZIFY BCD_Score
	TERM VERYPOOR := (6.5,0.0) (7.5,1.0) (9.0,1.0) ;
	TERM POOR := (5.31,0.0) (6.25,1.0) (7.22,0.0) ;
	TERM AVERAGE := (4.14,0.0) (5.0,1.0) (5.9,0.0) ;
	TERM VERYGOOD := (2.95,0.0) (3.75,1.0) (4.6,0.0) ;
	TERM EXCELLENT := (0.0,1.0) (2.5,1.0) (3.28,0.0) ;
END_FUZZIFY

FUZZIFY NSD_Score
	TERM VERYPOOR := (6.5,0.0) (7.5,1.0) (9.0,1.0) ;
	TERM POOR := (5.31,0.0) (6.25,1.0) (7.22,0.0) ;
	TERM AVERAGE := (4.14,0.0) (5.0,1.0) (5.9,0.0) ;
	TERM VERYGOOD := (2.95,0.0) (3.75,1.0) (4.6,0.0) ;
	TERM EXCELLENT := (0.0,1.0) (2.5,1.0) (3.28,0.0) ;
END_FUZZIFY

DEFUZZIFY score
	TERM VERYPOOR := (6.5,0.0) (7.5,1.0) (9.0,1.0) ;
	TERM POOR := (5.31,0.0) (6.25,1.0) (7.22,0.0) ;
	TERM AVERAGE := (4.14,0.0) (5.0,1.0) (5.9,0.0) ;
	TERM VERYGOOD := (2.95,0.0) (3.75,1.0) (4.6,0.0) ;
	TERM EXCELLENT := (0.0,1.0) (2.5,1.0) (3.28,0.0) ;
	METHOD : COG;
END_DEFUZZIFY

RULEBLOCK SCORE_RULES
	RULE 0 : IF BCD_Score IS EXCELLENT AND NSD_Score IS EXCELLENT THEN score IS EXCELLENT ;
	RULE 1 : IF BCD_Score IS EXCELLENT AND NSD_Score IS VERYGOOD THEN score IS VERYGOOD ;
	RULE 2 : IF BCD_Score IS VERYGOOD AND NSD_Score IS EXCELLENT THEN score IS VERYGOOD ;
	RULE 3 : IF BCD_Score IS VERYGOOD AND NSD_Score IS VERYGOOD THEN score IS VERYGOOD ;
	
	RULE 4 : IF BCD_Score IS VERYPOOR AND NSD_Score IS VERYPOOR THEN score IS VERYPOOR ;
	RULE 5 : IF BCD_Score IS VERYPOOR AND NSD_Score IS POOR THEN score IS VERYPOOR ;
	RULE 6 : IF BCD_Score IS POOR AND NSD_Score IS VERYPOOR THEN score IS VERYPOOR ;
	RULE 7 : IF BCD_Score IS POOR AND NSD_Score IS POOR THEN score IS POOR ;
	
	
	RULE 8 : IF BCD_Score IS AVERAGE AND NSD_Score IS AVERAGE THEN score IS AVERAGE ;
	
	RULE 9 : IF BCD_Score IS EXCELLENT AND NSD_Score IS AVERAGE THEN score IS EXCELLENT ;
	RULE 10 : IF BCD_Score IS VERYGOOD AND NSD_Score IS AVERAGE THEN score IS VERYGOOD ;	
	RULE 11 : IF BCD_Score IS AVERAGE AND NSD_Score IS EXCELLENT THEN score IS EXCELLENT ;
	RULE 12 : IF BCD_Score IS AVERAGE AND NSD_Score IS VERYGOOD THEN score IS VERYGOOD ;
	
	RULE 13 : IF BCD_Score IS VERYPOOR AND NSD_Score IS AVERAGE THEN score IS VERYPOOR ;
	RULE 14 : IF BCD_Score IS POOR AND NSD_Score IS AVERAGE THEN score IS POOR ;	
	RULE 15 : IF BCD_Score IS AVERAGE AND NSD_Score IS VERYPOOR THEN score IS VERYPOOR ;
	RULE 16 : IF BCD_Score IS AVERAGE AND NSD_Score IS POOR THEN score IS POOR ;
	
	RULE 17 : IF BCD_Score IS EXCELLENT AND NSD_Score IS VERYPOOR THEN score IS AVERAGE ;
	RULE 18 : IF BCD_Score IS EXCELLENT AND NSD_Score IS POOR THEN score IS AVERAGE ;
	RULE 19 : IF BCD_Score IS VERYGOOD AND NSD_Score IS VERYPOOR THEN score IS AVERAGE ;
	RULE 20 : IF BCD_Score IS VERYGOOD AND NSD_Score IS POOR THEN score IS AVERAGE ;
	
	RULE 21 : IF BCD_Score IS VERYPOOR AND NSD_Score IS EXCELLENT THEN score IS AVERAGE ;
	RULE 22 : IF BCD_Score IS VERYPOOR AND NSD_Score IS VERYGOOD THEN score IS AVERAGE ;
	RULE 23 : IF BCD_Score IS POOR AND NSD_Score IS EXCELLENT THEN score IS AVERAGE ;
	RULE 24 : IF BCD_Score IS POOR AND NSD_Score IS VERYGOOD THEN score IS AVERAGE ;
END_RULEBLOCK
END_FUNCTION_BLOCK
